{
  "name": "n8n-nodes-ksc-comfyui",
  "version": "1.0.4",
  "description": "n8n node to integrate with ComfyUI stable diffusion workflows for image transformations",
  "keywords": [
    "n8n-community-node-package",
    "comfyui",
    "stable-diffusion",
    "image-to-image",
    "image-to-video",
    "image-generation"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "christiankuri",
    "email": "mintedwealth.es@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StudioExitt/n8n-nodes-comfyui-image-to-image.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ComfyUIApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ComfyUI/ComfyuiImageToVideo.node.js",
      "dist/nodes/ComfyUI/KscComfyuiImageToImage.node.js"
    ]
  },
  "devDependencies": {
    "@types/form-data": "^2.5.2",
    "@types/node": "^22.14.1",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "form-data": "^4.0.0"
  }
}
